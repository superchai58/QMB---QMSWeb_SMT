@model QMSWeb.Model.QueryData

<script src="~/Scripts/QueryData/_PartialView_SpecialItem.js?v=20200829"></script>
<script src="~/Scripts/jquery.easyui.min.js"></script>

<div style="padding-left:5px">
    @{
         var xmlList = Model.xmlList;
         var jsonxmlList = Newtonsoft.Json.JsonConvert.SerializeObject(Model.xmlList);
         if (!string.IsNullOrEmpty(jsonxmlList))
         {
            foreach (var item in xmlList)
            { 
                string Item = item.Item;
                string ItemType = item.ItemType;
                @Html.Label("", Item, new { @class = "LabelBase" });
                if (Item == "WO")
                {
                  <input id="@Item" class="easyui-textbox TextBox_Q"/>
                  <button id="submitQ" name="btnQuery" class="ButtonQuery" onclick="btnLost_click(this.id)">查询</button>
                }
                else if (ItemType.ToLower() != "")
                {
                  string strItem=Item;
                  <input id="@strItem" class="easyui-textbox TextBox_Q"/>
                }
                <input id="xmlList" type="hidden" value="@jsonxmlList"/>
                <input id="QueryInterval" type="hidden" value="@Model.queryInterval"/> 
             }
          }                  
     }        
     <button id="submitF" name="btnFilter" class="ButtonFix" onclick="btnLost_click(this.id)">筛选</button>
     <button id="submitE" name="btnExcel" class="Buttonexport" onclick="btnLost_click(this.id)">导出</button>
</div>

<div style="width:100%;text-align:center;height:100%;">
    <div id="myDataTable_DIV" style="width:47%;display:inline-block;border:1px solid #b7d2ff;border-radius:1px;margin:5px">
        <Table id="myDataTable" class="table"></Table>
    </div>
    <div id="myDataTable_Filter_DIV" style="width:47%;display:inline-block;border:1px solid #b7d2ff;border-radius:1px;margin:5px">
        <Table id="myDataTable_Filter" class="table"></Table>
    </div>
</div>



